  "version": "0.0.1",
  "scripts": {
    "lint:eslint": "eslint --fix --ext .js,.svelte .",
    "lint:markdown": "markdownlint -i node_modules **/*.md",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,svelte,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{svelte,css}\"",
    "lint": "run-s lint:*",
    "prelint": "npm run build",
    "dev:app": "sapper dev",
    "dev:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "dev": "run-p dev:*",
    "build:app": "sapper build --legacy",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "run-s build:tailwind build:app",
    "export:app": "sapper export --legacy",
    "export": "run-s build:tailwind export:app",
    "postexport": "sitemap-static --pretty --prefix=https://thekeycap/ __sapper__/export > __sapper__/export/sitemap.xml",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
